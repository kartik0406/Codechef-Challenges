#include<bits/stdc++.h>
using namespace std;
#define lo long long


lo chefora_num(lo n){
    lo chefno=n;
    lo no=0;
    if(n<10){
        no=chefno;
    }
    else if(n>=10){
        n=n/10;
        while(n!=0){
            chefno=chefno*10+n%10;
            n=n/10;
        }
        no=chefno;
    }
    return no;
}
lo exonent_m(lo p, lo h ){
    lo result=1;
    while(h!=0){
        if(h%2==0){
            p=((p%1000000007)*(p%1000000007))%1000000007;
            h=floor(h/2);
        }
        else{
            result=((result%1000000007)*(p%1000000007))%1000000007;
            h=h-1;
        }
    }
    return result;
}
int main(){
   
    lo chef[100001]={0};
    lo arr[100001]={0};
    for(lo i=1;i<=100001;i++){
        chef[i]=chefora_num(i);
        arr[i]=arr[i-1]+chef[i];
    }
    lo t;
    cin>>t;
    while(t--){
        lo l,r;
        cin>>l>>r;
        lo d=arr[r]-arr[l];
        cout<<exonent_m(chef[l],d)<<"\n";
        
    }
    return 0;
}
